function _init()
 poke(0x5f15,133)
 poke(0x5f16,134)
 cnt=0
 scene.active(scene.title)
end

scene={
 active=function(s)
  s.init()
  _update=function()
   cnt=(cnt+1)%120
   --printh(cnt)
   s.update()
  end
  _draw=s.draw
 end
}

scene.title={
 init=function()
	 anim=0
	 fx0=16 fy0=22 fx1=fx0+95 fy1=fy0+47
	 offt=75
	 code="111011110110211101120212201201221011021120101211012012102"
 end,
 update=function()
  if anim==0 then
   offt=lerp(offt,0,.2)
   if (flr(offt)==0) anim=1
  elseif anim==1 then
   anim=2
  elseif anim==2 and btn(ðŸ…¾ï¸) then
   anim=3
  elseif anim==3 then
   offt=lerp(offt,-75,.2)
   if (flr(offt)==-75) anim=4
  elseif anim==4 then
	  fx0=lerp(fx0,0,.15)
	  fy0=lerp(fy0,0,.15)
	  fx1=lerp(fx1,128,.15)
	  fy1=lerp(fy1,128,.15)
	  if (flr(fx0)==flr(fy0) and flr(fx1)==flr(fy1)) scene.active(scene.game)
  end
 end,
 draw=function()
  cls(7)
  -- header & footer
  morse(code,7,3)
  morse(code,7,124)
  -- text
  spr12(1,22,82,1,2)
  print("press ðŸ…¾ï¸ to start",39,93,cnt%40>=20 and 5 or 7)
  if anim<2 then
   if (anim==1) fillp(0x5a5a.8)
   rectfill(22,82,105,105,7)
   fillp()
  end
  -- title
  frame(fx0,fy0,fx1,fy1)
  if offt<75 and offt>-74 then
   local offx=fx0+offt+17
   local offy=fy0+10
	  sspr(96,0,8,28,offx,offy) -- b1
	  sspr(107,0,14,28,offx+8,offy) -- b2
	  sspr(96,0,11,28,offx+25,offy) -- i
	  sspr(96,0,8,28,offx+40,offy) -- b1
	  sspr(107,0,14,28,offx+48,offy) --b2
  end
 end
}

scene.game={
 init=function()
  ply=59
  jump={flag=false,dy=-6,y=0}
  shot={flag=false}
  clouds={}
  enemies={}
  birds={}
 end,
 update=function()
  --jump
  if(btn(ðŸ…¾ï¸)) jump.flag=true
  if(jump.flag) jump.y+=jump.dy jump.dy+=1
  if(jump.dy>6) jump.flag=false jump.dy=-6
  --clouds
  for c in all(clouds) do
   c[1]-=.25
   if(c[1]<=-20) del(clouds,c)
  end
  if(cnt==0 and rnd()<.6 and #clouds<2) add(clouds,{124,flr(rnd(24))+4})
 end,
 draw=function()
  cls(7)
  frame(0,0,127,127)
  print("ðŸ…¾ï¸ to jump\nâŽ to shot",44,98,5)
  -- rail
  for i=0,10 do
	  spr12(26,4+i*12-(cnt*1.5)%4,76)
	 end
  spr12(30,4,62,4,2) -- train
  sspr(73,3,10,9,40,ply+jump.y) -- player
  foreach(clouds, function(c) spr12(22,c[1],c[2],2,1) end) --clouds
 end
}
